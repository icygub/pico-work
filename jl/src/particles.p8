__lua__
-- particle demo
-- v1.0 by atlas
-- (i.e. jon pierson)
-- <admin@kinix.net> :)
-- [kinix.net]: game design 4 u!

-- initilizing variables
x=64
y=64
ctr=0
particles={}

-- reset cursor and delete particles
function reset()
	x=64
	y=64
	for p in all(particles) do
		del(particles,p)
	end
end

function _init()
	reset()
end

function gen_particle(col, rad, min_size, max_size, speed)
	part = {
		lx=x + (flr(rnd(2*rad)) - rad),
		ly=y + (flr(rnd(2*rad)) - rad),
		s=min_size+flr(rnd(max_size - min_size)),
		c=col,
		spd=speed
	}
	add(particles,part)

end

function _update()
	if btn(4) then
		reset()
	end
	-- particles generated by pressing x
	-- size s sets radius r for particles
	-- color is set with var c
	-- part added to particles{}
	if btn(5) then
		gen_particle(flr(rnd(5))+1, 40, 2, 4, .1)
	end
	if btn(0) then
		if x>1 then
			x-=1
		end
	end
	if btn(1) then
		if x<127 then
			x+=1
		end
	end
	if btn(2) then
		if y>1 then
			y-=1
		end
	end
	if btn(3) then
		if y<127 then
			y+=1
		end
	end
	if ctr==5 then
	-- particles generated by counter overflow
	-- size s sets radius r for particles
	-- color is set with var c
	-- part added to particles{}
		ctr=0
		part = {
			lx=x + (flr(rnd(16)) - 8),
			ly=y + (flr(rnd(16)) - 8),
			s=2+flr(rnd(2)),
			c=1,
			spd=.1
		}
		add(particles,part)
	else
		ctr+=1
	end

end

function _draw()
	cls()
	for p in all(particles) do
		circfill(p.lx,p.ly,p.s,p.c)
		if p.s>0 then
			p.s -= p.spd
		else
			del(particles,p)
		end
	end
	circfill(x,y,1,7)
	print("press — for alt. color",20,110,5)
	print("press ƒ to reset",32,118,5)
end
